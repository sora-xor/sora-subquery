@Library('jenkins-library')

def jobParams  = [
    string(defaultValue: '18015500', name: 'startBlock', trim: true),
    string(defaultValue: '1', name: 'startBlockStg', trim: true),
    string(defaultValue: '20000', name: 'queryTimeout', trim: true),
    string(defaultValue: '100', name: 'batchSize', trim: true),
    booleanParam(defaultValue: false, name: 'fullClean'),
    booleanParam(defaultValue: false, name: 'createProject'),
    booleanParam(defaultValue: false, name: 'stagingSlot'),
    booleanParam(defaultValue: true, name: 'prodSlot')
]

def pipeline = new org.subqd.AppPipeline(
    steps: this,
    validate: true,
    jobParams: jobParams
)
pipeline.runPipeline()
